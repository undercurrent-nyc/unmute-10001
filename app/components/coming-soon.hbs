<div class="w-full h-screen bg-black flex flex-col justify-between text-white">
  <header class="flex-grow-0 w-full p-10 flex flex-row justify-between">
    <div class="flex-grow-0 h-16">
      <a href="https://undercurrent.nyc">
        <p class="sr-only">Undercurrent Website</p>
        {{svg-jar "undercurrent" class="fill-current h-10 md:h-16"}}
      </a>
    </div>
    <div class="pl-10 flex flex-grow flex-col">
      <div class="flex w-full justify-end text-white">
          {{svg-jar "header-small" class="fill-current h-10 md:h-12"}}
      </div>
    </div>
  </header>
  

  <div class="w-full flex-grow flex flex-row justify-end content-center px-10 pb-10">
    <div class="w-full md:w-2/3 flex flex-col justify-center align-end space-y-5 md:space-y-12"
         {{did-insert this.breathe }}>
      <h1 class="sr-only">10001, Coming Soon from Undercurrent</h1>
      {{svg-jar "coming-soon" class="coming-soon-svg w-full fill-current"}}
      <h2 class="text-white text-2xl md:text-4xl font-bold uppercase text-right">
        Launching 08/05/20
      </h2>
    </div>
  </div>
</div>

